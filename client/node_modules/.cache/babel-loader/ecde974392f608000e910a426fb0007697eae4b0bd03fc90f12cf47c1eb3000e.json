n       } else {\n         this.$message.error(\"获取数据库列表失败\")\n       }\n     },\n    onSearch() {\n\n    },\n    getBackupList(){\n      this.$request.getDatabaseBackup(data).then(res=>{\n        if (res.status === 200){\n          this.backupList = res.data.data\n        }\n      })\n    },\n    getModelStatus(status) {\n      this.openAddScriptModel = status\n    },\n\n    async getDatabaseList() {\n      await this.$request.getDatabaseList().then(res => {\n        if (res.status === 200) {\n          this.dbList = res.data.data\n          this.getDatabaseListFlag = true\n        } else {\n          this.getDatabaseListFlag = false\n        }\n      })\n\n    }\n  }\n}\n</script>\n\n<style scoped>\n.my-b-button {\n  margin-top: 10px;\n  display: flex;\n  justify-content: space-between;\n}\n.my-s-table {\n  margin-top: 10px;\n}\n</style>\n"],"mappings":"AAgCA,OAAAA,SAAA;AACA,SAAAC,eAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAH;EAAA;EACAI,KAAA;IACA;MACAC,kBAAA;MACAC,MAAA;MACAC,mBAAA;MACAC,OAAA,GACA;QAAAC,KAAA;QAAAC,SAAA;QAAAC,GAAA;MAAA,GACA;QAAAF,KAAA;QAAAC,SAAA;QAAAC,GAAA;MAAA,GACA;QAAAF,KAAA;QAAAC,SAAA;QAAAC,GAAA;MAAA,GACA;QAAAF,KAAA;QAAAC,SAAA;QAAAC,GAAA;MAAA,GACA;QAAAF,KAAA;QAAAC,SAAA;QAAAC,GAAA;QAAAC,WAAA;UAAAC,YAAA;QAAA;MAAA,EACA;MACAC,UAAA;IACA;EACA;EACAC,QAAA,GAEA;EACAC,OAAA;IACA,MAAAC,OAAA;MACA,WAAAhB,eAAA;MAEA,SAAAM,mBAAA;QACA,KAAAF,kBAAA;MACA;QACA,KAAAa,QAAA,CAAAC,KAAA;MACA;IACA;IACAC,SAAA,GAEA;IACAC,cAAA;MACA,KAAAC,QAAA,CAAAC,iBAAA,CAAAnB,IAAA,EAAAoB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,MAAA;UACA,KAAAZ,UAAA,GAAAW,GAAA,CAAArB,IAAA,CAAAA,IAAA;QACA;MACA;IACA;IACAuB,eAAAD,MAAA;MACA,KAAArB,kBAAA,GAAAqB,MAAA;IACA;IAEA,MAAAzB,gBAAA;MACA,WAAAqB,QAAA,CAAArB,eAAA,GAAAuB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,MAAA;UACA,KAAApB,MAAA,GAAAmB,GAAA,CAAArB,IAAA,CAAAA,IAAA;UACA,KAAAG,mBAAA;QACA;UACA,KAAAA,mBAAA;QACA;MACA;IAEA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}