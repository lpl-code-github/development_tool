{"ast":null,"code":"import Markdown from \"@/components/code/Markdown\";\nexport default {\n  name: 'SlateGenerator',\n  components: {\n    Markdown\n  },\n  data() {\n    return {\n      docContent: \"\",\n      loading: false,\n      confirmIsShow: false\n    };\n  },\n  created() {\n    this.$bus.$on('markdownInput', newVal => {\n      this.docContent = newVal;\n    });\n  },\n  mounted() {\n    var editDoc = sessionStorage.getItem(\"edit_doc\");\n    if (editDoc !== null && editDoc !== \"\") {\n      this.docContent = editDoc;\n    }\n  },\n  methods: {\n    getDefaultClick() {\n      if (this.docContent !== \"\") {\n        this.confirmIsShow = true;\n      }\n    },\n    getDefaultDoc() {\n      this.loading = true;\n      this.$request.getDefaultSlateDoc().then(res => {\n        if (res.status === 200) {\n          this.docContent = res.data;\n        }\n        this.loading = false;\n      });\n    },\n    confirm(e) {\n      console.log(e);\n      this.$message.success('Click on Yes');\n    },\n    cancel(e) {\n      console.log(e);\n      this.$message.error('Click on No');\n    }\n  }\n};","map":{"version":3,"names":["Markdown","name","components","data","docContent","loading","confirmIsShow","created","$bus","$on","newVal","mounted","editDoc","sessionStorage","getItem","methods","getDefaultClick","getDefaultDoc","$request","getDefaultSlateDoc","then","res","status","confirm","e","console","log","$message","success","cancel","error"],"sources":["src/views/SlateGenerator.vue"],"sourcesContent":["<template>\n  <div>\n    <a-popconfirm\n        title=\"编辑器中还有内容，确认重新获取模版吗？\"\n        ok-text=\"Yes\"\n        cancel-text=\"No\"\n        @confirm=\"confirm\"\n        @cancel=\"cancel\"\n        :disabled = \"confirmIsShow\"\n    >\n      <a-button style=\"margin-top: 20px\" @click=\"getDefaultClick\">获取默认模版</a-button>\n    </a-popconfirm>\n\n    <a-spin size=\"large\" :spinning=\"loading\">\n      <Markdown :doc-content=\"docContent\"></Markdown>\n    </a-spin>\n\n  </div>\n</template>\n\n<script>\nimport Markdown from \"@/components/code/Markdown\";\nexport default {\n  name: 'SlateGenerator',\n  components: {Markdown},\n  data() {\n    return {\n      docContent:\"\",\n      loading: false,\n      confirmIsShow:false\n    }\n  },\n  created() {\n    this.$bus.$on('markdownInput', (newVal) => {\n      this.docContent = newVal\n    });\n  },\n  mounted() {\n    var editDoc = sessionStorage.getItem(\"edit_doc\");\n    if (editDoc !== null && editDoc !==\"\"){\n      this.docContent = editDoc\n    }\n  },\n  methods: {\n    getDefaultClick() {\n      if (this.docContent !== \"\"){\n        this.confirmIsShow = true\n      }\n\n    },\n    getDefaultDoc(){\n      this.loading = true\n      this.$request.getDefaultSlateDoc().then(res => {\n        if (res.status === 200) {\n          this.docContent = res.data\n        }\n        this.loading = false\n      })\n    },\n    confirm(e) {\n      console.log(e);\n      this.$message.success('Click on Yes');\n    },\n    cancel(e) {\n      console.log(e);\n      this.$message.error('Click on No');\n    },\n  }\n};\n</script>\n<style scoped>\n\n\n</style>\n"],"mappings":"AAqBA,OAAAA,QAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAF;EAAA;EACAG,KAAA;IACA;MACAC,UAAA;MACAC,OAAA;MACAC,aAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA,CAAAC,GAAA,kBAAAC,MAAA;MACA,KAAAN,UAAA,GAAAM,MAAA;IACA;EACA;EACAC,QAAA;IACA,IAAAC,OAAA,GAAAC,cAAA,CAAAC,OAAA;IACA,IAAAF,OAAA,aAAAA,OAAA;MACA,KAAAR,UAAA,GAAAQ,OAAA;IACA;EACA;EACAG,OAAA;IACAC,gBAAA;MACA,SAAAZ,UAAA;QACA,KAAAE,aAAA;MACA;IAEA;IACAW,cAAA;MACA,KAAAZ,OAAA;MACA,KAAAa,QAAA,CAAAC,kBAAA,GAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,MAAA;UACA,KAAAlB,UAAA,GAAAiB,GAAA,CAAAlB,IAAA;QACA;QACA,KAAAE,OAAA;MACA;IACA;IACAkB,QAAAC,CAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,CAAA;MACA,KAAAG,QAAA,CAAAC,OAAA;IACA;IACAC,OAAAL,CAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,CAAA;MACA,KAAAG,QAAA,CAAAG,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}