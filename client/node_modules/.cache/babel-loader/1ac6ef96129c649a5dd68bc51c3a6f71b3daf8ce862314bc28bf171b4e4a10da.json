 },\n    handleSelectChange(value) {\n      console.log(value);\n      this.form.setFieldsValue({\n        note: `Hi, ${value === 'male' ? 'man' : 'lady'}!`,\n      });\n    },\n    handleOk(e) {\n      this.loading = true;\n      e.preventDefault();\n      this.form.validateFields((err, values) => {\n        if (!err) {\n          console.log('Received values of form: ', values);\n          this.loading = false;\n        }\n      });\n\n      setTimeout(() => {\n        this.$emit('updateModelStatus', false)\n        this.loading = false;\n      }, 3000);\n    },\n    handleCancel(e) {\n      this.$emit('updateModelStatus', false)\n    },\n    afterClose(e){\n      this.$emit('updateModelStatus', false)\n    }\n  },\n};\n</script>\n"],"mappings":"AA4CA;EACAA,IAAA;EACAC,KAAA;IACAC,QAAA;MACAC,IAAA,EAAAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,OAAA;MACAC,UAAA;MACAC,IAAA,OAAAC,KAAA,CAAAC,UAAA;QAAAZ,IAAA;MAAA;IACA;EACA;EACAa,KAAA;IACAX,QAAA;MACAY,OAAA,WAAAA,CAAAC,MAAA,EAAAC,MAAA;QACAC,OAAA,CAAAC,GAAA,CAAAH,MAAA;QACA,KAAAP,OAAA,GAAAO,MAAA;MACA;MACA;MACAI,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAZ,OAAA,QAAAN,QAAA;EACA;EACAmB,OAAA;IACAC,aAAAC,CAAA,GAEA;IACAC,mBAAAC,KAAA;MACAR,OAAA,CAAAC,GAAA,CAAAO,KAAA;MACA,KAAAf,IAAA,CAAAgB,cAAA;QACAC,IAAA,SAAAF,KAAA;MACA;IACA;IACAG,SAAAL,CAAA;MACA,KAAAhB,OAAA;MACAgB,CAAA,CAAAM,cAAA;MACA,KAAAnB,IAAA,CAAAoB,cAAA,EAAAC,GAAA,EAAAC,MAAA;QACA,KAAAD,GAAA;UACAd,OAAA,CAAAC,GAAA,8BAAAc,MAAA;UACA,KAAAzB,OAAA;QACA;MACA;MAEA0B,UAAA;QACA,KAAAC,KAAA;QACA,KAAA3B,OAAA;MACA;IACA;IACA4B,aAAAZ,CAAA;MACA,KAAAW,KAAA;IACA;IACAE,WAAAb,CAAA;MACA,KAAAW,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}