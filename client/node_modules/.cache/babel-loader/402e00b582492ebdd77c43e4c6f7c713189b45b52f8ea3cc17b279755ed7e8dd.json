{"ast":null,"code":"export default {\n  name: \"SwitchComponent\",\n  props: {\n    flag: {\n      type: Boolean,\n      required: true\n    },\n    tooltipText: {\n      type: String,\n      required: true\n    },\n    switchText: {\n      type: String,\n      required: true\n    },\n    type: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      loading: false,\n      checked: false\n    };\n  },\n  created() {\n    this.checked = this.flag;\n  },\n  methods: {\n    handleSwitch() {\n      var checked = this.checked;\n      this.$request.switchApi(this.type, checked).then(res => {\n        if (res.status !== 200) {\n          this.checked = !checked;\n        } else {\n          if (res.data.data.handle) {\n            if (this.checked) this.$message.success(this.switchText + ': 操作成功', 2.5);\n          } else {\n            this.$message.error(this.switchText + ': 操作失败', 2.5);\n          }\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","props","flag","type","Boolean","required","tooltipText","String","switchText","data","loading","checked","created","methods","handleSwitch","$request","switchApi","then","res","status","handle","$message","success","error"],"sources":["src/components/switch/SwitchComponent.vue"],"sourcesContent":["<template>\n  <div class=\"my-switch\">\n    <a-tooltip placement=\"left\">\n      <template slot=\"title\">\n        {{ tooltipText }}\n      </template>\n      {{ switchText }}\n    </a-tooltip>\n    <a-switch v-model=\"checked\" @click=\"handleSwitch\">\n      <a-icon slot=\"checkedChildren\" type=\"check\" />\n      <a-icon slot=\"unCheckedChildren\" type=\"close\" />\n    </a-switch>\n    <br />\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"SwitchComponent\",\n  props: {\n    flag: {\n      type: Boolean,\n      required: true\n    },\n    tooltipText: {\n      type: String,\n      required: true\n    },\n    switchText: {\n      type: String,\n      required: true\n    },\n    type: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      loading: false,\n      checked: false\n    };\n  },\n  created() {\n    this.checked = this.flag\n  },\n  methods: {\n    handleSwitch() {\n      var checked = this.checked;\n      this.$request.switchApi(this.type, checked).then(res => {\n        if (res.status !== 200) {\n          this.checked = !checked;\n        }else {\n          if (res.data.data.handle){\n            if (this.checked)\n            this.$message.success(this.switchText+': 操作成功', 2.5)\n          }else {\n            this.$message.error(this.switchText+': 操作失败', 2.5)\n          }\n        }\n      });\n    }\n  }\n};\n</script>\n\n<style scoped>\n\n</style>\n"],"mappings":"AAiBA;EACAA,IAAA;EACAC,KAAA;IACAC,IAAA;MACAC,IAAA,EAAAC,OAAA;MACAC,QAAA;IACA;IACAC,WAAA;MACAH,IAAA,EAAAI,MAAA;MACAF,QAAA;IACA;IACAG,UAAA;MACAL,IAAA,EAAAI,MAAA;MACAF,QAAA;IACA;IACAF,IAAA;MACAA,IAAA,EAAAI,MAAA;MACAF,QAAA;IACA;EACA;EACAI,KAAA;IACA;MACAC,OAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAD,OAAA,QAAAT,IAAA;EACA;EACAW,OAAA;IACAC,aAAA;MACA,IAAAH,OAAA,QAAAA,OAAA;MACA,KAAAI,QAAA,CAAAC,SAAA,MAAAb,IAAA,EAAAQ,OAAA,EAAAM,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,MAAA;UACA,KAAAR,OAAA,IAAAA,OAAA;QACA;UACA,IAAAO,GAAA,CAAAT,IAAA,CAAAA,IAAA,CAAAW,MAAA;YACA,SAAAT,OAAA,EACA,KAAAU,QAAA,CAAAC,OAAA,MAAAd,UAAA;UACA;YACA,KAAAa,QAAA,CAAAE,KAAA,MAAAf,UAAA;UACA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}