{"ast":null,"code":"export default {\n  name: \"AddScript\",\n  props: {\n    openFlag: {\n      type: Boolean,\n      required: true\n    },\n    filterOption: {\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      loading: false,\n      visible: false,\n      formLayout: 'horizontal',\n      form: this.$form.createForm(this, {\n        name: 'coordinated'\n      }),\n      tags: [],\n      selectedItems: [] // 被选中的\n    };\n  },\n\n  watch: {\n    openFlag: {\n      handler: function (newVal, oldVal) {\n        this.visible = newVal;\n      },\n      // 深度观察监听\n      deep: true\n    },\n    filterOption: {\n      handler: function (newVal, oldVal) {\n        this.tags = newVal;\n      },\n      // 深度观察监听\n      deep: true\n    }\n  },\n  computed: {\n    filteredOptions() {\n      return this.tags.filter(o => !this.selectedItems.includes(o));\n    }\n  },\n  mounted() {\n    this.visible = this.openFlag;\n    this.tags = this.filterOption;\n  },\n  methods: {\n    handleSelect(selectedItems) {\n      this.selectedItems = selectedItems;\n    },\n    handleOk(e) {\n      this.loading = true;\n      e.preventDefault();\n      this.form.validateFields((err, values) => {\n        if (!err) {\n          console.log('Received values of form: ', values);\n        } else {\n          this.loading = false;\n        }\n      });\n      setTimeout(() => {\n        this.$emit('updateModelStatus', false);\n        this.loading = false;\n      }, 3000);\n    },\n    handleCancel(e) {\n      this.$emit('updateModelStatus', false);\n    },\n    afterClose(e) {\n      this.$emit('updateModelStatus', false);\n    },\n    handleChange(info) {\n      const status = info.file.status;\n      if (status !== 'uploading') {\n        console.log(info.file, info.fileList);\n      }\n      if (status === 'done') {\n        this.$message.success(`${info.file.name} file uploaded successfully.`);\n      } else if (status === 'error') {\n        this.$message.error(`${info.file.name} file upload failed.`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","props","openFlag","type","Boolean","required","filterOption","Array","data","loading","visible","formLayout","form","$form","createForm","tags","selectedItems","watch","handler","newVal","oldVal","deep","computed","filteredOptions","filter","o","includes","mounted","methods","handleSelect","handleOk","e","preventDefault","validateFields","err","values","console","log","setTimeout","$emit","handleCancel","afterClose","handleChange","info","status","file","fileList","$message","success","error"],"sources":["src/components/Script/AddScript.vue"],"sourcesContent":["<template>\n  <div>\n    <a-modal v-model=\"visible\" title=\"Title\" on-ok=\"handleOk\" :afterClose=\"afterClose\">\n      <template slot=\"footer\">\n        <a-button key=\"back\" @click=\"handleCancel\">\n          取消\n        </a-button>\n        <a-button key=\"submit\" type=\"primary\" :loading=\"loading\" @click=\"handleOk\">\n          提交\n        </a-button>\n      </template>\n      <a-form :form=\"form\" :label-col=\"{ span: 3 }\" :wrapper-col=\"{ span: 20}\" style=\"width: 100%\">\n        <a-form-item label=\"名称\">\n          <a-input\n              v-decorator=\"['name', { rules: [{ required: true, message: 'Please input your name!' }] }]\"\n          />\n        </a-form-item>\n        <a-form-model-item label=\"描述\" prop=\"desc\">\n          <a-input  v-model=\"form.desc\" type=\"textarea\" />\n        </a-form-model-item>\n        <a-form-model-item label=\"DB\" prop=\"db\">\n          <a-select\n              show-search\n              placeholder=\"选择数据库\"\n              mode=\"multiple\"\n              option-filter-prop=\"children\"\n              :filter-option=\"filterOption\"\n              @change=\"handleSelect\"\n          >\n            <a-select-option v-for=\"(item,index) in tags\" :key=\"index\" :value=\"item\">\n              {{ item.name }}\n            </a-select-option>\n          </a-select>\n        </a-form-model-item>\n\n      </a-form>\n      <div style=\"margin: auto;width: 93%\">\n        <a-upload-dragger\n            :directory=\"false\"\n            name=\"file\"\n            :multiple=\"false\"\n            action=\"https://www.mocky.io/v2/5cc8019d300000980a055e76\"\n            @change=\"handleChange\"\n        >\n          <p class=\"ant-upload-drag-icon\">\n            <a-icon type=\"inbox\" />\n          </p>\n          <p class=\"ant-upload-text\">\n            单击或拖动文件到此区域进行上传\n          </p>\n          <p class=\"ant-upload-hint\">\n            只接受一个脚本文件\n          </p>\n        </a-upload-dragger>\n      </div>\n    </a-modal>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"AddScript\",\n  props: {\n    openFlag: {\n      type: Boolean,\n      required: true\n    },\n    filterOption:{\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      loading: false,\n      visible: false,\n      formLayout: 'horizontal',\n      form: this.$form.createForm(this, { name: 'coordinated' }),\n      tags:[],\n      selectedItems: [], // 被选中的\n    };\n  },\n  watch:{\n    openFlag:{\n      handler: function (newVal,oldVal) {\n        this.visible = newVal\n      },\n      // 深度观察监听\n      deep: true\n    },\n    filterOption:{\n      handler: function (newVal,oldVal) {\n        this.tags = newVal\n      },\n      // 深度观察监听\n      deep: true\n    }\n  },\n  computed: {\n    filteredOptions() {\n      return this.tags.filter(o => !this.selectedItems.includes(o));\n    },\n  },\n  mounted() {\n    this.visible = this.openFlag\n    this.tags = this.filterOption\n  },\n\n  methods: {\n    handleSelect(selectedItems) {\n      this.selectedItems = selectedItems;\n    },\n\n    handleOk(e) {\n      this.loading = true;\n      e.preventDefault();\n      this.form.validateFields((err, values) => {\n        if (!err) {\n          console.log('Received values of form: ', values);\n        }else {\n          this.loading = false;\n        }\n      });\n\n      setTimeout(() => {\n        this.$emit('updateModelStatus', false)\n        this.loading = false;\n      }, 3000);\n    },\n    handleCancel(e) {\n      this.$emit('updateModelStatus', false)\n    },\n    afterClose(e){\n      this.$emit('updateModelStatus', false)\n    },\n    handleChange(info) {\n      const status = info.file.status;\n      if (status !== 'uploading') {\n        console.log(info.file, info.fileList);\n      }\n      if (status === 'done') {\n        this.$message.success(`${info.file.name} file uploaded successfully.`);\n      } else if (status === 'error') {\n        this.$message.error(`${info.file.name} file upload failed.`);\n      }\n    },\n  },\n};\n</script>\n"],"mappings":"AA2DA;EACAA,IAAA;EACAC,KAAA;IACAC,QAAA;MACAC,IAAA,EAAAC,OAAA;MACAC,QAAA;IACA;IACAC,YAAA;MACAH,IAAA,EAAAI,KAAA;MACAF,QAAA;IACA;EACA;EACAG,KAAA;IACA;MACAC,OAAA;MACAC,OAAA;MACAC,UAAA;MACAC,IAAA,OAAAC,KAAA,CAAAC,UAAA;QAAAd,IAAA;MAAA;MACAe,IAAA;MACAC,aAAA;IACA;EACA;;EACAC,KAAA;IACAf,QAAA;MACAgB,OAAA,WAAAA,CAAAC,MAAA,EAAAC,MAAA;QACA,KAAAV,OAAA,GAAAS,MAAA;MACA;MACA;MACAE,IAAA;IACA;IACAf,YAAA;MACAY,OAAA,WAAAA,CAAAC,MAAA,EAAAC,MAAA;QACA,KAAAL,IAAA,GAAAI,MAAA;MACA;MACA;MACAE,IAAA;IACA;EACA;EACAC,QAAA;IACAC,gBAAA;MACA,YAAAR,IAAA,CAAAS,MAAA,CAAAC,CAAA,UAAAT,aAAA,CAAAU,QAAA,CAAAD,CAAA;IACA;EACA;EACAE,QAAA;IACA,KAAAjB,OAAA,QAAAR,QAAA;IACA,KAAAa,IAAA,QAAAT,YAAA;EACA;EAEAsB,OAAA;IACAC,aAAAb,aAAA;MACA,KAAAA,aAAA,GAAAA,aAAA;IACA;IAEAc,SAAAC,CAAA;MACA,KAAAtB,OAAA;MACAsB,CAAA,CAAAC,cAAA;MACA,KAAApB,IAAA,CAAAqB,cAAA,EAAAC,GAAA,EAAAC,MAAA;QACA,KAAAD,GAAA;UACAE,OAAA,CAAAC,GAAA,8BAAAF,MAAA;QACA;UACA,KAAA1B,OAAA;QACA;MACA;MAEA6B,UAAA;QACA,KAAAC,KAAA;QACA,KAAA9B,OAAA;MACA;IACA;IACA+B,aAAAT,CAAA;MACA,KAAAQ,KAAA;IACA;IACAE,WAAAV,CAAA;MACA,KAAAQ,KAAA;IACA;IACAG,aAAAC,IAAA;MACA,MAAAC,MAAA,GAAAD,IAAA,CAAAE,IAAA,CAAAD,MAAA;MACA,IAAAA,MAAA;QACAR,OAAA,CAAAC,GAAA,CAAAM,IAAA,CAAAE,IAAA,EAAAF,IAAA,CAAAG,QAAA;MACA;MACA,IAAAF,MAAA;QACA,KAAA